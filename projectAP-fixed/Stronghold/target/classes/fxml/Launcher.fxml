<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="720.0" prefWidth="1240.0" styleClass="somePain" stylesheets="@../CSS/Launcher.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Network.LaunchInitializer">
   <children>
      <Label fx:id="IsHost" layoutX="10.0" layoutY="10.0" text="Host" textFill="#ffc8c8">
         <effect>
            <DropShadow color="#426f08" spread="0.82" />
         </effect>
         <font>
            <Font name="Cambria Bold" size="25.0" />
         </font>
      </Label>
      <VBox fx:id="ServerVbox" layoutX="918.0" layoutY="30.0" prefHeight="57.0" prefWidth="279.0" visible="false">
         <children>
            <Label text="Server Address" textFill="#ffc8c8">
               <font>
                  <Font name="Cambria Bold" size="19.0" />
               </font>
               <effect>
                  <DropShadow color="#426f08" spread="0.82" />
               </effect>
            </Label>
            <TextField fx:id="SeverAddress" prefHeight="31.0" prefWidth="214.0" promptText="ex:  8080" />
         </children>
      </VBox>
      <Button fx:id="exit" layoutX="14.0" layoutY="675.0" mnemonicParsing="false" onAction="#BackAction" prefWidth="100.0" text="exit" textFill="#ffc8c8">
         <font>
            <Font name="Cambria Bold" size="19.0" />
         </font>
      </Button>
      <Button fx:id="next" layoutX="1097.0" layoutY="675.0" mnemonicParsing="false" onAction="#NextAction" prefWidth="100.0" text="next" textFill="#ffc8c8">
         <font>
            <Font name="Cambria Bold" size="19.0" />
         </font>
      </Button>
      <VBox fx:id="MapVbox" layoutX="918.0" layoutY="94.0" prefHeight="57.0" prefWidth="279.0" visible="false">
         <children>
            <Label fx:id="ServerAddress1" text="Map " textFill="#ffc8c8">
               <effect>
                  <DropShadow color="#426f08" spread="0.82" />
               </effect>
               <font>
                  <Font name="Cambria Bold" size="19.0" />
               </font>
            </Label>
            <TextField fx:id="mapNumber" prefHeight="31.0" prefWidth="214.0" promptText="ex:  firstMap" />
         </children>
      </VBox>
      <HBox fx:id="LogHbox" alignment="CENTER" layoutX="798.0" layoutY="59.0" prefHeight="31.0" prefWidth="380.0" spacing="15.0" visible="false">
         <children>
            <Label text="Server Address:" textFill="#ffc8c8">
               <effect>
                  <DropShadow color="#426f08" spread="0.82" />
               </effect>
               <font>
                  <Font name="Cambria Bold" size="21.0" />
               </font>
            </Label>
            <Label fx:id="AnnouncedAddress" text="AnnouncedAddress" textFill="#13462d">
               <font>
                  <Font name="Cambria Bold" size="16.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <VBox fx:id="PlayersList" layoutX="828.0" layoutY="132.0" prefHeight="200.0" prefWidth="330.0" spacing="10.0" style="-fx-border-color: green; -fx-padding: 7; -fx-border-width: 3; -fx-border-radius: 15;" visible="false">
         <children>
            <Label fx:id="ServerAddress11" text="Players:" textFill="#ffc8c8">
               <effect>
                  <DropShadow color="#426f08" spread="0.82" />
               </effect>
               <font>
                  <Font name="Cambria Bold" size="19.0" />
               </font>
            </Label>
         </children></VBox>
      <HBox fx:id="LogHboxMap" alignment="CENTER" layoutX="798.0" layoutY="94.0" prefHeight="31.0" prefWidth="380.0" spacing="15.0" visible="false">
         <children>
            <Label text="Map:" textFill="#ffc8c8">
               <effect>
                  <DropShadow color="#426f08" spread="0.82" />
               </effect>
               <font>
                  <Font name="Cambria Bold" size="21.0" />
               </font>
            </Label>
            <Label fx:id="MapLabel" text="AnnouncedMap" textFill="#13462d">
               <font>
                  <Font name="Cambria Bold" size="16.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Button fx:id="startGame" layoutX="1097.0" layoutY="675.0" mnemonicParsing="false" onAction="#startAction" prefWidth="100.0" text="start" textFill="#ffc8c8" visible="false">
         <font>
            <Font name="Cambria Bold" size="19.0" />
         </font>
      </Button>
   </children>
</Pane>
