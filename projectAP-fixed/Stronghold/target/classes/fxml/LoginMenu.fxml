<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<AnchorPane prefHeight="750.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.fxmlMenu.LoginMenuFxml">
   <children>
      <HBox>
         <VBox alignment="CENTER">
            <TextField fx:id="userField"  promptText="username" />
            <PasswordField fx:id="passField" promptText="password" />
            <Button fx:id="enterButton" mnemonicParsing="false" text="enter" onMouseClicked="#enterGame"/>
            <Button fx:id="forgotPassword" mnemonicParsing="false" text="forgot password" onMouseClicked="#forgotPass"/>
         </VBox>
      </HBox>

   </children>
</AnchorPane>
