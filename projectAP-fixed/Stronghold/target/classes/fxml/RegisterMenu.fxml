<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<Pane prefHeight="750.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Controllers.RegisterController">
   <children>
      <VBox alignment="CENTER" layoutX="30.0" layoutY="114.0" prefHeight="446.0" prefWidth="284.0">
         <children>
            <VBox alignment="CENTER_LEFT" prefHeight="87.0" prefWidth="242.0" spacing="8.0">
               <children>
                  <TextField fx:id="username" onInputMethodTextChanged="#usernameChanged" promptText="username" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="88.0" prefWidth="242.0" spacing="8.0">
               <children>
                  <PasswordField fx:id="password" promptText="password" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="117.0" prefWidth="242.0" spacing="8.0">
               <children>
                  <TextField fx:id="nickname" prefHeight="31.0" prefWidth="255.0" promptText="nickname" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="77.0" prefWidth="242.0" spacing="8.0">
               <children>
                  <TextField fx:id="email" promptText="email" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
               </children>
            </VBox>
            <HBox alignment="CENTER_LEFT" prefHeight="84.0" prefWidth="242.0" spacing="8.0">
               <children>
                  <CheckBox mnemonicParsing="false" prefHeight="21.0" prefWidth="34.0" />
                  <VBox prefHeight="83.0" prefWidth="203.0" spacing="8.0">
                     <children>
                        <TextField fx:id="slogan" prefHeight="31.0" prefWidth="203.0" promptText="slogan" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="CENTER" layoutX="801.0" layoutY="634.0" prefHeight="67.0" prefWidth="365.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#reset" prefWidth="100.0" text="reset">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#back" prefWidth="100.0" text="back">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#next" prefWidth="100.0" text="next">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <ScrollPane layoutX="26.0" layoutY="585.0" prefHeight="116.0" prefWidth="612.0" />
   </children>
</Pane>
