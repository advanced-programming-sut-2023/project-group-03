            <Label fx:id="ServerAddress" text="Server Address" textFill="#ffc8c8">
               <font>
                  <Font name="Cambria Bold" size="19.0" />
               </font>
               <effect>
                  <DropShadow color="#426f08" spread="0.82" />
               </effect>
            </Label>
            <TextField fx:id="SeverAddress" prefHeight="31.0" prefWidth="214.0" promptText="ex:  1234.567.890" />
         </children>
      </VBox>
      <Button fx:id="exit" layoutX="14.0" layoutY="675.0" mnemonicParsing="false" prefWidth="100.0" text="exit" textFill="#ffc8c8">
         <font>
            <Font name="Cambria Bold" size="19.0" />
         </font>
      </Button>
      <Button fx:id="next" layoutX="1097.0" layoutY="675.0" mnemonicParsing="false" prefWidth="100.0" text="next" textFill="#ffc8c8">
         <font>
            <Font name="Cambria Bold" size="19.0" />
         </font>
      </Button>
      <VBox fx:id="MapVbox" layoutX="918.0" layoutY="30.0" prefHeight="57.0" prefWidth="279.0" visible="false">
