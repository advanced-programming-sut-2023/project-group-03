            <Label fx:id="ServerAddress1" text="Map " textFill="#ffc8c8">
               <effect>
                  <DropShadow color="#426f08" spread="0.82" />
               </effect>
               <font>
                  <Font name="Cambria Bold" size="19.0" />
               </font>
            </Label>
            <TextField fx:id="mapNumber" prefHeight="31.0" prefWidth="214.0" promptText="ex:  firstMap" />
         </children>
      </VBox>
      <HBox alignment="CENTER" layoutX="767.0" layoutY="59.0" prefHeight="31.0" prefWidth="380.0" spacing="15.0">
         <children>
            <Label text="Server Address:" textFill="#ffc8c8">
               <effect>
                  <DropShadow color="#426f08" spread="0.82" />
               </effect>
               <font>
                  <Font name="Cambria Bold" size="21.0" />
               </font>
            </Label>
            <Label text="AnnouncedAddress" textFill="#13462d">
               <font>
                  <Font name="Cambria Bold" size="16.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
