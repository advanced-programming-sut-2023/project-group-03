<AnchorPane prefHeight="720.0" prefWidth="1240.0" styleClass="LOGIN" stylesheets="@../CSS/login.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.fxmlMenu.LoginMenuFxml">
   <children>
      <HBox layoutX="92.0" layoutY="21.0" prefHeight="149.0" prefWidth="236.0">
         <children>
            <VBox alignment="CENTER">
               <children>
                  <TextField fx:id="userField" promptText="username" />
                  <PasswordField fx:id="passField" promptText="password" />
                  <Button fx:id="enterButton" mnemonicParsing="false" onMouseClicked="#enterGame" text="enter" />
                  <Button fx:id="forgotPassword" mnemonicParsing="false" onMouseClicked="#forgotPass" text="forgot password" />
               </children>
            </VBox>
         </children>
      </HBox>

   </children>
</AnchorPane>
