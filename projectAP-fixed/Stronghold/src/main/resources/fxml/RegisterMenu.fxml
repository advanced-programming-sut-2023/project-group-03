<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="750.0" prefWidth="1200.0" styleClass="EndingPane" stylesheets="@../CSS/Register.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Controllers.RegisterController">
   <children>
      <VBox alignment="CENTER" layoutX="30.0" layoutY="114.0" prefHeight="446.0" prefWidth="284.0">
         <children>
            <VBox alignment="CENTER_LEFT" prefHeight="87.0" prefWidth="242.0" spacing="8.0">
               <children>
                  <TextField fx:id="username" onInputMethodTextChanged="#usernameChanged" onKeyTyped="#usernameResponse" promptText="username" />
                  <Text fx:id="usernameAns" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="username is empty" />
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="88.0" prefWidth="242.0" spacing="8.0">
               <children>
                  <CheckBox fx:id="showSlogan1" mnemonicParsing="false" onAction="#showSlagan" prefHeight="21.0" prefWidth="107.0" text="random" textFill="#d4ff00" />
                  <PasswordField fx:id="password" onKeyTyped="#passwordResponse" promptText="password" />
                  <Text fx:id="PassAns" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="password is empty" />
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="117.0" prefWidth="242.0" spacing="8.0">
               <children>
                  <TextField fx:id="nickname" onKeyReleased="#nicknameResponse" prefHeight="31.0" prefWidth="255.0" promptText="nickname" />
                  <Text fx:id="nickAns" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="nickname is empty" />
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT" prefHeight="77.0" prefWidth="242.0" spacing="8.0">
               <children>
                  <TextField fx:id="email" onKeyTyped="#emailResponse" promptText="email" />
                  <Text fx:id="emailAns" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="email is empty" />
               </children>
            </VBox>
            <HBox alignment="CENTER_LEFT" prefHeight="84.0" prefWidth="242.0" spacing="8.0">
               <children>
                  <CheckBox fx:id="showSlogan" mnemonicParsing="false" onAction="#showSlagan" prefHeight="21.0" prefWidth="34.0" />
                  <VBox prefHeight="83.0" prefWidth="203.0" spacing="8.0">
                     <children>
                        <TextField fx:id="slogan" onKeyTyped="#sloganRespose" prefHeight="31.0" prefWidth="203.0" promptText="slogan" visible="false" />
                        <Text fx:id="SloganAns" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="slogan is empty" visible="false" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="CENTER" layoutX="801.0" layoutY="634.0" prefHeight="67.0" prefWidth="365.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#reset" prefWidth="100.0" text="reset">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#back" prefWidth="100.0" text="back">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#next" prefWidth="100.0" text="next">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <ScrollPane fx:id="scroller" layoutX="26.0" layoutY="585.0" prefHeight="116.0" prefWidth="612.0" />
   </children>
</Pane>
